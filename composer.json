{
  "name": "joeri-abbo/laravel-api-markdown-tree",
  "description": "A Laravel package to generate a tree view of API routes in Markdown format.",
  "type": "library",
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.5.0",
    "orchestra/testbench": "^8.0.0",
    "phpunit/phpunit": "^10.0.0"
  },
  "autoload": {
    "psr-4": {
      "JoeriAbbo\\LaravelApiMarkdownTree\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "dev-master"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}

